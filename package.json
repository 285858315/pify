  },
  "files": [
    "index.js",
    "proxy.js"
  ],
  "keywords": [
    "promise",
    "promises",
    "promisify",
    "all",
    "denodify",
    "denodeify",
    "callback",
    "cb",
    "node",
    "then",
    "thenify",
    "convert",
    "transform",
    "wrap",
    "wrapper",
    "bind",
    "to",
    "async",
    "es2015",
    "bluebird"
  ],
  "devDependencies": {
    "ava": "*",
    "pinkie-promise": "^2.0.0",
    "v8-natives": "0.0.3",
    "xo": "*"
  },
  "xo": {
    "esnext": true
  }
}
